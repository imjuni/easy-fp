{
  "name": "my-easy-fp",
  "version": "0.10.0",
  "description": "Simple functional programming utility & Misc programming tool",
  "scripts": {
    "script": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.json ts-node -r tsconfig-paths/register",
    "lint": "eslint --quiet --ext ts,tsx ./src/*",
    "clean": "rimraf dist artifact",
    "ctix": "ctix create -p tsconfig.json",
    "prepub:prod": "npm run ctix && npm run build",
    "pub:prod": "npm publish --access=public",
    "postpub:prod": "ctix clean -p tsconfig.json",
    "build:tsc": "node_modules/.bin/tsc --noemit -p tsconfig.json",
    "prebuild": "npm run clean && npm run ctix",
    "build": "cross-env NODE_ENV=develop npx webpack --config webpack.config.prod.js",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=develop npx webpack --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:imjuni/my-easy-fp.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/my-easy-fp/issues"
  },
  "homepage": "https://github.com/imjuni/my-easy-fp#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "ctix": "^0.3.3",
    "debug": "^4.3.2",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
